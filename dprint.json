{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "projectType": "openSource",
  "plugins": [
    "https://plugins.dprint.dev/json-0.17.4.wasm"
  ],
  "includes": ["**/*.json"],
  "excludes": ["target/**", ".git/**", "node_modules/**"],
  "json": {
    "indentWidth": 2,
    "trailingCommas": "none"
  },
  "yaml": {
    "lineWidth": 100
  }
}
